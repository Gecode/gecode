  template <class A>
  IntArrayArg*
  Arg::newIntArray(const A& a) {
    IntArrayArg* ret = Arg::newIntArray(a.size());
    for (int i=a.size(); i--;)
      (*ret)[i] = a[i];
    return ret;
  }
  
